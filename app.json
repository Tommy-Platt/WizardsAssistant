{
  "expo": {
    "name": "wizardsAssistant",
    "slug": "wizardsAssistant",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/WA-logo.png",
    "scheme": "wizardsassistant",
    "deepLinking": true,
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tplatt.wizardsassistant"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tplatt.wizardsassistant",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "wizardsassistant"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/WA-logo-64.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/WA-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.785004448257-1krp0hsltbs80nq1h6pcdtfdr8nadtvk "
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2226466e-ee5f-4e45-bc0d-81e866caaab4"
      }
    }
  }
}
